<%-include('partials/css.ejs')%>



<%-include('partials/navbar.ejs')%>
<%-include('partials/hero.ejs')%>

<section class="bsb-blog-5 py-3">
  <div class="container">
    <div class="row justify-content-md-center">
      <div class="col-12 col-md-10 col-lg-8 col-xl-7 col-xxl-6">
        <h2 class="display-5 text-center">
          Latest Job Post 
        </h2>
        <hr class="w-50 mx-auto mb-5 mb-xl-9 border-dark-subtle" />
      </div>
    </div>
  </div>

  
    <div class="container d-flex flex-wrap justify-content-around">
      <!-- post -->
    
        <% if(jobpost.length > 0) { for(let i = 0; i < jobpost.length; i++) { %>
          <div class="card border-0 shadow-lg my-4 mx-3" style="width: 420px;">
            <figure class="card-img-top mb-4">
              <a href="#!">
                <img
                  class="img-fluid bsb-scale bsb-hover-scale-up"
                  loading="lazy"
                  src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRL3NH836uj1_Z0oe5NRkdzpFQQNQYWyKtEuA&s"
                  alt="Job Thumbnail"
                  style="object-fit: cover; height: 200px; width: 100%; border-bottom: 1px solid #e6e6e6;"
                />
              </a>
            </figure>
          
            <div class="card-body p-4">
              <h5 class="card-title font-weight-bold text-dark mb-3">
                <a href="#!" class="text-dark text-decoration-none">
                  <%=jobpost[i].TITLE%>
                </a>
              </h5>
              
              <p class="text-muted fs-6 mb-3">
                <%=jobpost[i].DESCRIPTION%>
              </p>
          
              <div class="d-flex flex-wrap justify-content-between align-items-center mb-3">
                <div class="badge bg-primary text-white rounded-pill fs-6 px-3 py-2">
                  <%=jobpost[i].TYPE%>
                </div>
                <div class="d-flex align-items-center text-secondary fs-6">
                  <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-building" viewBox="0 0 16 16">
                    <path d="M2 2v12h12V2H2zm10 11H4v-1h8v1zm0-3H4V9h8v1zm0-3H4V6h8v1zm0-3H4V3h8v1z"/>
                  </svg>
                  <span class="ms-2"><%=jobpost[i].COMPANY%> </span>
                </div>
              </div>
          
              <div class="d-flex flex-wrap justify-content-between align-items-center">
                <% if(user) {%>
                  <a href="/page/apply/<%=jobpost[i].ID%>/<%=user.ID%>" class="text-light text-decoration-none fs-6 bg-dark px-3 rounded-pill">
                    Apply Now
                  </a>
                <%}%>
                <div class="d-flex align-items-center text-muted fs-7">
                  <svg xmlns="http://www.w3.org/2000/svg" width="14" height="14" fill="currentColor" class="bi bi-calendar3" viewBox="0 0 16 16">
                    <path d="M14 0H2a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2zM1 3.857C1 3.384 1.448 3 2 3h12c.552 0 1 .384 1 .857v10.286c0 .473-.448.857-1 .857H2c-.552 0-1-.384-1-.857V3.857z"/>
                  </svg>
                  <span class="ms-2">Posted on <%=jobpost[i].DATE%></span>
                </div>
              </div>
            </div>
          </div>
          
        <% } } else { %>
        <div class="card border-0 bg-transparent">
          <figure class="card-img-top mb-4 overflow-hidden bsb-overlay-hover">
            <a href="#!">
              <img
                class="img-fluid bsb-scale bsb-hover-scale-up"
                loading="lazy"
                src="jobpost:image/jpeg;base64,"
                alt="Living"
              />
            </a>
            <figcaption>
              <svg
                xmlns="http://www.w3.org/2000/svg"
                width="32"
                height="32"
                fill="currentColor"
                class="bi bi-eye text-white bsb-hover-fadeInLeft"
                viewBox="0 0 16 16"
              >
                <path
                  d="M16 8s-3-5.5-8-5.5S0 8 0 8s3 5.5 8 5.5S16 8 16 8zM1.173 8a13.133 13.133 0 0 1 1.66-2.043C4.12 4.668 5.88 3.5 8 3.5c2.12 0 3.879 1.168 5.168 2.457A13.133 13.133 0 0 1 14.828 8c-.058.087-.122.183-.195.288-.335.48-.83 1.12-1.465 1.755C11.879 11.332 10.119 12.5 8 12.5c-2.12 0-3.879-1.168-5.168-2.457A13.134 13.134 0 0 1 1.172 8z"
                />
                <path
                  d="M8 5.5a2.5 2.5 0 1 0 0 5 2.5 2.5 0 0 0 0-5zM4.5 8a3.5 3.5 0 1 1 7 0 3.5 3.5 0 0 1-7 0z"
                />
              </svg>
              <h4 class="h6 text-white bsb-hover-fadeInRight mt-2">
                Read More
              </h4>
            </figcaption>
          </figure>
          <div class="card-body m-0 p-0">
            <div class="entry-header mb-3">
              <ul class="entry-meta list-unstyled d-flex mb-3">
                <li>
                  <a
                    class="d-inline-flex px-2 py-1 link-primary text-primary-emphasis bg-primary-subtle border border-primary-subtle rounded-2 text-decoration-none fs-7"
                    href="#!"
                    >Living</a
                  >
                </li>
              </ul>
              <h2 class="card-title entry-title h4 m-0">
                <a class="link-dark text-decoration-none" href="#!"
                  >How to Use Facial Expressions in Public Speaking</a
                >
              </h2>
            </div>
          </div>
          <div class="card-footer border-0 bg-transparent p-0 m-0">
            <ul class="entry-meta list-unstyled d-flex align-items-center m-0">
              <li>
                <a
                  class="fs-7 link-secondary text-decoration-none d-flex align-items-center"
                  href="#!"
                >
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="14"
                    height="14"
                    fill="currentColor"
                    class="bi bi-calendar3"
                    viewBox="0 0 16 16"
                  >
                    <path
                      d="M14 0H2a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2zM1 3.857C1 3.384 1.448 3 2 3h12c.552 0 1 .384 1 .857v10.286c0 .473-.448.857-1 .857H2c-.552 0-1-.384-1-.857V3.857z"
                    />
                    <path
                      d="M6.5 7a1 1 0 1 0 0-2 1 1 0 0 0 0 2zm3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2zm3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2zm-9 3a1 1 0 1 0 0-2 1 1 0 0 0 0 2zm3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2zm3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2zm3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2zm-9 3a1 1 0 1 0 0-2 1 1 0 0 0 0 2zm3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2zm3 0a1 1 0 1 0 0-2 1 1 0 0 0 0 2z"
                    />
                  </svg>
                  <span class="ms-2 fs-7">11 Dec 2030</span>
                </a>
              </li>
              <li>
                <span class="px-3">&bull;</span>
              </li>
              <li>
                <a
                  class="link-secondary text-decoration-none d-flex align-items-center"
                  href="#!"
                >
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="14"
                    height="14"
                    fill="currentColor"
                    class="bi bi-chat-dots"
                    viewBox="0 0 16 16"
                  >
                    <path
                      d="M5 8a1 1 0 1 1-2 0 1 1 0 0 1 2 0zm4 0a1 1 0 1 1-2 0 1 1 0 0 1 2 0zm3 1a1 1 0 1 0 0-2 1 1 0 0 0 0 2z"
                    />
                    <path
                      d="m2.165 15.803.02-.004c1.83-.363 2.948-.842 3.468-1.105A9.06 9.06 0 0 0 8 15c4.418 0 8-3.134 8-7s-3.582-7-8-7-8 3.134-8 7c0 1.76.743 3.37 1.97 4.6a10.437 10.437 0 0 1-.524 2.318l-.003.011a10.722 10.722 0 0 1-.244.637c-.079.186.074.394.273.362a21.673 21.673 0 0 0 .693-.125zm.8-3.108a1 1 0 0 0-.287-.801C1.618 10.83 1 9.468 1 8c0-3.192 3.004-6 7-6s7 2.808 7 6c0 3.193-3.004 6-7 6a8.06 8.06 0 0 1-2.088-.272 1 1 0 0 0-.711.074c-.387.196-1.24.57-2.634.893a10.97 10.97 0 0 0 .398-2z"
                    />
                  </svg>
                  <span class="ms-2 fs-7">383</span>
                </a>
              </li>
            </ul>
          </div>
        </div>
        <%}%>
    
    </div>
 
</section>


<%-include('./partials/footer.ejs')%>